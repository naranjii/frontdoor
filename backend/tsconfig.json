  "skipLibCheck": true,
  "forceConsistentCasingInFileNames": true
  }
  ,
  "include": [
    "src/**/*.ts",
    "tests/**/*.ts",
    "tests/**/*.d.ts"
  ]
  ,
  "exclude": [
